- if user_signed_in?

  = render partial: "header"
  .contents
    .contents__pickup
      .contents__pickup__slide
        .contents__pickup__slide__text 海外旅行特集
      .contents__pickup__sub
        %ul
          %li ハワイ
          %li ドイツ
          %li オーストラリア
          %li インド
    .contents__main
      .contents__main__tab
        .contents__main__tab__all 総合
        .contents__main__tab__date 日程
      .contents__main__sort
        .contents__main__sort__recommend オススメ順
        .contents__main__sort__project-status 受付状態別
      .contents__main__projects
        = render partial: "trip", collection: @projects, as: "project"

  = render partial: "footer"

- else
  = render partial: "contents-not-sign-in"

